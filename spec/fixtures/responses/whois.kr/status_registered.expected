#status
  %s == :registered

#available?
  %s == false

#registered?
  %s == true


#created_on
  %s %CLASS{time}
  %s %TIME{2007-03-02}

#updated_on
  %s %CLASS{time}
  %s %TIME{2010-10-04}

#expires_on
  %s %CLASS{time}
  %s %TIME{2015-03-02}


#nameservers
  %s %CLASS{array}
  %s %SIZE{2}
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.google.com"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.google.com"
