#status
  %s == :registered

#available?
  %s == false

#registered?
  %s == true


#created_on
  %s %CLASS{time}
  %s %TIME{2006-04-13}

#updated_on
  %s %CLASS{time}
  %s %TIME{2010-03-26}

#expires_on
  %s %CLASS{time}
  %s %TIME{2011-05-01}


#nameservers
  %s %CLASS{array}
  %s %SIZE{2}
  %s[0] %CLASS{nameserver}
  %s[0].name == "ns1.google.com"
  %s[1] %CLASS{nameserver}
  %s[1].name == "ns2.google.com"
